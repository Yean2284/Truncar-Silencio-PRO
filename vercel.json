{
  "version": 2,
  "builds": [
    {
      "src": "api/process.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/process.py"
    }
  ],
  "functions": {
    "api/process.py": {
      "memory": 3008,
      "maxDuration": 300
    }
  },
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
